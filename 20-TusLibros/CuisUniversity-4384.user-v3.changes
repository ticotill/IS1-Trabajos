checkout: aCartID charging: aCreditCard 
	
	| cart  |
	cart := carts at: aCartID.
	
	cashier := Cashier 
		toCheckout: cart 
		charging: aCreditCard
		throught: merchantProcessor 
		on:  today
		registeringOn: salesBook.
		
	cashier checkOut.
	
	^1
	
	
	! !
